<mapper namespace="com.yz_mentalhealth.department.dao.DepartmentMapper">

    <select id="getDepartments"
            parameterType="com.yz_mentalhealth.department.entity.Department"
            resultType="com.yz_mentalhealth.department.entity.Department">
        SELECT
          ID,
          DEPARTMENTNAME,
          CJR,
          CJSJ,
          XGR,
          XGSJ
        FROM DEPARTMENT_INFO
    </select>

</mapper>