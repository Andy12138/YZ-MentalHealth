<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yz_mentalhealth.login.dao.LoginMapper">
    <select id="isLogin" parameterType="com.yz_mentalhealth.user.entity.User" resultType="int">
        SELECT count(1) FROM user_info WHERE usercode=#{usercode} and password=#{password}
    </select>

</mapper>